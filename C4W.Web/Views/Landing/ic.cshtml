                        INICIA <br />
                        @ViewBag.Fecha
                    </div>
                    <div class="hlwidget bblue nohover margin-bot">
                        @ViewBag.Ciudad <br />
                        <small>@ViewBag.Pais</small>
                    </div>
                </div>

