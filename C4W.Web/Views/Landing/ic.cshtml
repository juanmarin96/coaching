}

<div class="sslide sslide-2 sslide sslide-2-2 sslide sslide-2-2">
    <div class="pinkbg">
        <div class="container">
            <h2>SEMINARIO</h2>
            <h1>INTELIGENCIA CONVERSACIONAL</h1>
            <br />
            <div class="row to-bottom">
                <div class="col-sm-4 col-sm-push-8 widget-container">
                    <div class="empty-widget bblue nohover" id="form-container">
                        @using (Html.BeginForm("Send", "Landing", FormMethod.Post, new { @class = "form-horizontal", role = "form", @id = "form" }))
                        {
                            <div class="col-xs-12">
                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
